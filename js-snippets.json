      "     param1: 1,",
      "     param2: 2,",
      "     param3: 3,",
      "   });",
      "   const url = BASE_URL + END_POINT + '?' + params;",
      "  ",
      "   const options = {",
      "     headers: {",
      "      header1: 1,",
      "      header2: 2,",
      "      header3: 3,",
      "     },",
      "   };",
      "  ",
